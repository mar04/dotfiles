# bind-key <key> <operation> [<dialog>]
# unbind-key <key> [<dialog>]

unbind-key l
unbind-key A mark-feed-read
unbind-key C mark-all-feeds-read
unbind-key j
unbind-key k
unbind-key ^K
unbind-key o
unbind-key N
unbind-key J
unbind-key K

bind-key l open
bind-key h quit articlelist
bind-key h quit article
bind-key M mark-feed-read
bind-key ^m mark-all-feeds-read
bind-key j next
bind-key k prev
bind-key o open-in-browser-and-mark-read
bind-key m toggle-article-read
bind-key J next-feed
bind-key K prev-feed
bind-key k up
bind-key j down

bind-key i macro-prefix

macro i set browser "w3m %u" ; open-in-browser-and-mark-read ; set browser "firefox %u"
macro d set browser "urxvtc -e mpv %u" ; open-in-browser-and-mark-read ; set browser "firefox %u"
